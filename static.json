{
  "https_only": true,
  "headers": {
    "/*": {
      "X-Author": "sergeypetrov5",
      "Access-Control-Allow-Origin": "*"
    }, 
    "/login/*": {
      "Access-Control-Allow-Origin": "*"
    }, 
    "/sample/*": {
      "Access-Control-Allow-Origin": "*"
    }
  },
  "routes": {
    "/login/*": "login.txt",
    "/sample/*": "sample.txt"
  },
  "redirects": {
    "/login": {
      "url": "/login/",
      "status": 301
    },
    "/sample": {
      "url": "/sample/",
      "status": 301
    }
  }
}
